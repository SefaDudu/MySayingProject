@model IDataResult<List<Publisher>>
@*
    For more information on enabling MVC for empty projects, visit https://go.microsoft.com/fwlink/?LinkID=397860
*@
@{
}

<a href="/Book/Modify/" class="btn btn-danger " style="margin-bottom:10px;">YENİ KİTAP</a>

<div class="col-8"> 
    <table id="example" class="table table-striped table-bordered" style="width:100%">
    <thead>
        <tr>
            <th>ID</th>
            <th>Name</th>

            <th>
                Sil
            </th>
            <th>Güncelle</th>

        </tr>
    </thead>
    <tbody>
        @foreach (Publisher x in Model.Data)
        {


            <tr>

                <td>@x.ID</td>
                <td>@x.Name</td>

                <td><a href="/Publisher/Delete/@x.ID" class="btn btn-danger">SİL</a></td>
                <td><a href="/Publisher/Modify/@x.ID" class="btn btn-primary">GÜNCELLE</a></td>
            </tr>
        }
    </tbody>
</table> 
 </div>
<div class="col-4">   </div>



